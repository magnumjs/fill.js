{
  "name": "fill.js",
  "version": "1.0.0",
  "description": "Simple and fun JSON to HTML templating",
  "main": "src/main.js",
  "unpkg": "dist/fill.min.js",
  "sideEffects": false,
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "files": [
    "dist",
    "lib",
    "!dist/index.html"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "build": "babel src -d lib && webpack --config config/webpack.prod.config",
    "prepublishOnly": "webpack --config config/webpack.prod.config",
    "start": "webpack-dev-server --config config/webpack.dev.config.js",
    "prepare": "npm run build",
    "format": "prettier --write 'src/**/*.{js,scss}'",
    "lint": "eslint --quiet src --ext .js",
    "test": "jest",
    "verify": "npm-run-all format lint",
    "verify-save": "npm-run-all format lint test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/magnumjs/fill.js.git"
  },
  "keywords": [
    "javascript",
    "templating",
    "json",
    "dom"
  ],
  "author": "Michael Glazer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/magnumjs/fill.js/issues"
  },
  "homepage": "https://github.com/magnumjs/fill.js#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.2.0",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^24.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.1.4"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run verify"
    }
  }
}
